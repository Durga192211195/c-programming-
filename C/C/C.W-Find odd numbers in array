#include <stdio.h>

int main() {
    int arr[100], n, i, odd = 0;
    printf("Enter the size of the array: ");
    scanf("%d", &n);
    printf("Enter the elements of the array:\n");
    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        odd ^= arr[i];
    }
    printf("The odd element in the array is: %d\n", odd);
    return 0;
}
